<Window x:Class="DyDesktopWinForms.VideoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DyDesktopWinForms"
        mc:Ignorable="d"
        Title="VideoWindow" Height="450" Width="800">
    <Grid Background="Black">
        <MediaElement x:Name="mediaElement" LoadedBehavior="Manual" Stretch="UniformToFill" MediaEnded="mediaElement_MediaEnded" />

    </Grid>
</Window>
